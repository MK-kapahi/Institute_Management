<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="700.0" minWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="status.ViewStatusController">
   <children>
      <ComboBox fx:id="ComboTech" layoutX="73.0" layoutY="128.0" prefWidth="150.0" promptText="Select Technology" style="-fx-background-color: white;" />
      <RadioButton fx:id="radiofull" layoutX="275.0" layoutY="132.0" mnemonicParsing="false" style="-fx-background-color: white;" text="Fully Paid">
         <font>
            <Font size="15.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="radiobal" layoutX="406.0" layoutY="130.0" mnemonicParsing="false" style="-fx-background-color: white;" text="Balance">
         <font>
            <Font size="15.0" />
         </font>
      </RadioButton>
      <Button layoutX="547.0" layoutY="125.0" mnemonicParsing="false" onAction="#doSearch" style="-fx-background-color: black;" text="Search" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TableView fx:id="table" layoutX="73.0" layoutY="242.0" prefHeight="385.0" prefWidth="520.0" style="-fx-background-color: white;" />
      <Button layoutX="587.0" layoutY="650.0" mnemonicParsing="false" onAction="#goback" style="-fx-background-color: black;" text="Return" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="2.0" layoutY="7.0" prefHeight="69.0" prefWidth="694.0" text="Fee Status">
         <font>
            <Font size="29.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
   </children>
</AnchorPane>
